mixin btn(data)
  #{data.tag}.btn( type= data.type ? data.type : null class= data.cls ? data.cls : 'btn-primary' href= data.href ? data.href : null data-id= data.dataId ? data.dataId : null)
    if data.title
      span!= data.title
    if data.icon
      svg(width='11' height='10' role='img' aria-hidden='true' focusable='false')
        use(xlink:href=`./assets/sprite.svg#icon-${data.icon}`)